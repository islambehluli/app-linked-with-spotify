<div class="tracks-wrapper">
    <div class="tracks-container">
        {{#each data.body.items }}
            <div class="track-lines-div">
                <p>{{ artists.[0].name }}: {{ name }}</p>
                <audio controls>
                    <source src="{{ preview_url }}">
                </audio>
            </div>
        {{/each}}
    </div>
</div>